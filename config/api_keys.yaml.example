# API Keys Configuration
# Copy this file to api_keys.yaml and fill in your keys
# NEVER commit api_keys.yaml to version control

# ===== 미국 주식 =====
fmp:
  api_key: "YOUR_FMP_API_KEY"
  # Get your key at: https://site.financialmodelingprep.com/developer/docs

# ===== 거시경제 지표 =====
fred:
  api_key: "YOUR_FRED_API_KEY"
  # Get your key at: https://fred.stlouisfed.org/docs/api/api_key.html

# ===== 국내 주식 =====
kis:
  app_key: "YOUR_KIS_APP_KEY"
  app_secret: "YOUR_KIS_APP_SECRET"
  account_no: "YOUR_ACCOUNT_NUMBER"    # 계좌번호 (8자리)
  account_code: "01"                    # 01: 위탁, 02: 연금저축
  is_paper: true                        # true: 모의투자, false: 실전투자
  # Get your keys at: https://apiportal.koreainvestment.com

kiwoom:
  app_key: "YOUR_KIWOOM_APP_KEY"
  app_secret: "YOUR_KIWOOM_APP_SECRET"
  account_no: "YOUR_ACCOUNT_NUMBER"
  is_paper: true                        # true: 모의투자, false: 실전투자
  # Get your keys at: https://openapi.kiwoom.com

# ===== 암호화폐 =====
binance:
  api_key: "YOUR_BINANCE_API_KEY"
  api_secret: "YOUR_BINANCE_SECRET"
  testnet: true                         # true: 테스트넷, false: 실거래
  # Get your keys at: https://www.binance.com/en/my/settings/api-management

upbit:
  access_key: "YOUR_UPBIT_ACCESS_KEY"
  secret_key: "YOUR_UPBIT_SECRET_KEY"
  # Get your keys at: https://upbit.com/mypage/open_api_management
